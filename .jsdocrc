{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 0,
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./.gh-pages/",
    "encoding": "utf8",
    "readme": "./README.md",
    "recurse": true,
    "template": "node_modules/docdash",
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "layoutFile": "node_modules/docdash/tmpl/layout.tmpl",
      "includeDate": false
    }
  },
  "docdash": {
    "collapse": true,
    "includeDate": false,
    "typedefs": true,
    "private": false,
    "removeQuotes": true,
    "search": true,
    "menu": {
      "GitHub": {
        "href": "https://github.com/phtmlorg/phtml"
      },
      "Support": {
        "href": "https://gitter.im/postcss"
      }
    },
    "favicon": "favicon.ico",
    "scripts": [
      "styles/custom.css"
    ],
		"meta": {
			"title": "pHTML",
			"description": "A tool for transforming HTML with JavaScript"
		},
		"openGraph": {
			"title": "pHTML",
			"type": "website",
			"image": "https://phtml.io/phtml.png",
			"site_name": "Docdash",
			"url": "https://phtml.io/"
		}
  }
}
